@inject NavigationManager NavigationManager
@code {   
    
    [Parameter]
    public string Login { get; set; } = "/Identity/Account/Login";
    
    protected override void OnInitialized()
    {
        Redirect();
    }

    public void Redirect()
    {
        try
        {
            NavigationManager.NavigateTo(Login, true);
        }
        catch(Exception e)
        {
            NavigationManager.NavigateTo(Login, true);
        }
    }
}
